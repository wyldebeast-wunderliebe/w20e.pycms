<html xmlns="http://www.w3.org/1999/xhtml" 
      xml:lang="en" 
      xmlns:tal="http://xml.zope.org/namespaces/tal"
      xmlns:metal="http://xml.zope.org/namespaces/metal"
      metal:use-macro="admin.macros['master']">

  <head>
    <metal:fill-slot fill-slot="extra_js_slot">
      <script type="text/javascript" src="/eye_static/jquery.js"></script>
      <script type="text/javascript" src="/eye_static/jquery.ui.js"></script>
      <script type="text/javascript" src="/eye_static/jquery.layout.js"></script>
      <script src="/eye_static/jquery.dynatree.js" type="text/javascript"></script>
      <script type="text/javascript" src="/eye_static/eye.js"></script>
    </metal:fill-slot>

    <metal:fill-slot fill-slot="extra_css_slot">
      <!--link href="/eye_static/eye.css" rel="stylesheet" type="text/css"-->
      <link href="/eye_static/ui.dynatree.css" rel="stylesheet" type="text/css">
    </metal:fill-slot>

  </head>
  
  <body>

    <div id="zodbbrowser" metal:fill-slot="content">

      <div class="ui-layout-north">
        <a href="http://github.com/davisagli/eye" target="_blank">
          Eye
        </a>
      </div>
      
      <div class="ui-layout-west">
        <div id="tree"></div>
      </div>
      
      <div class="ui-layout-center">
        <div id="info"></div>
      </div>
    </div>
      
  </body>
</html>
